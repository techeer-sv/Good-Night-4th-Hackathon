{
  "name": "svelte-app",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "dev": "vite dev",
    "build": "vite build",
    "preview": "vite preview",
    "playwright:install": "playwright install",
    "test:e2e": "playwright test",
    "test:e2e:headed": "playwright test --headed",
    "test:e2e:ci": "playwright test --reporter=list",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "lint": "prettier --plugin-search-dir . --check . && eslint .",
    "format": "prettier --plugin-search-dir . --write .",
    "test": "vitest",
    "test:run": "vitest run",
    "test:diag": "vitest run --reporter=default --reporter=hanging-process",
    "test:browser": "vitest --browser",
    "test:browser:run": "vitest run --browser",
    "db:seed": "bun src/lib/server/db/seed.ts"
  },
  "devDependencies": {
    "@axe-core/playwright": "^4.10.2",
    "@fullhuman/postcss-purgecss": "^7.0.2",
    "@playwright/test": "^1.55.0",
    "@sveltejs/adapter-auto": "6.1.0",
    "@sveltejs/kit": "^2.36.1",
    "@sveltejs/vite-plugin-svelte": "^6.1.3",
    "@testing-library/svelte": "^5.2.8",
    "@typescript-eslint/eslint-plugin": "8.39.1",
    "@typescript-eslint/parser": "^8.40.0",
    "autoprefixer": "^10.4.21",
    "drizzle-kit": "^0.31.4",
    "eslint": "^9.33.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-svelte": "^3.11.0",
    "playwright": "^1.55.0",
    "postcss": "^8.5.6",
    "prettier": "^3.6.2",
    "prettier-plugin-svelte": "^3.4.0",
    "purgecss": "^7.0.2",
    "purgecss-from-svelte": "^2.0.2",
    "svelte": "^5.38.2",
    "svelte-check": "^4.3.1",
    "tailwindcss": "^3.4.17",
    "tslib": "^2.8.1",
    "typescript": "^5.9.2",
    "vite": "^7.1.3"
  },
  "type": "module",
  "dependencies": {
    "@inlang/paraglide-js": "^2.2.0",
    "@libsql/client": "^0.15.12",
    "@node-rs/argon2": "^2.0.2",
    "@oslojs/crypto": "^1.0.1",
    "@oslojs/encoding": "^1.1.0",
    "@storybook/addon-svelte-csf": "^5.0.8",
    "@tailwindcss/container-queries": "^0.1.1",
    "@tailwindcss/forms": "^0.5.10",
    "@tailwindcss/postcss": "^4.1.12",
    "@tailwindcss/vite": "^4.1.12",
    "@vitest/browser": "^3.2.4",
    "drizzle-orm": "^0.44.4",
    "looks-same": "^10.0.1",
    "pngjs": "^7.0.0",
    "storybook": "^9.1.3",
    "vite-plugin-devtools-json": "^1.0.0",
    "vitest": "^3.2.4",
    "vitest-browser-svelte": "^1.1.0"
  }
}
